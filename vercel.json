{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/login",
      "dest": "index.js"
    },
    {
      "src": "/signup",
      "dest": "index.js"
    },
    {
      "src": "/tasks",
      "dest": "index.js"
    },
    {
      "src": "/newTask",
      "dest": "index.js"
    },
    {
      "src": "/profile",
      "dest": "index.js"
    },
    {
      "src": "/developer",
      "dest": "index.js"
    },
    {
      "src": "/contactUs",
      "dest": "index.js"
    },
    {
      "src": "/about",
      "dest": "index.js"
    },
    
    {
      "src": "/",
      "dest": "build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "build/$1"
    }
  ]
}